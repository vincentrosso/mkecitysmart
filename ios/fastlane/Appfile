# Fill values via environment variables to avoid checking secrets into git.
# Examples:
#   APP_IDENTIFIER=com.mkecitysmart.app
#   APPLE_ID=you@example.com
#   ITC_TEAM_ID=123456789
#   TEAM_ID=ABCDE12345

app_identifier(ENV['APP_IDENTIFIER'] || 'com.example.app')
apple_id(ENV['APPLE_ID'] || 'you@example.com')
itc_team_id(ENV['ITC_TEAM_ID']) if ENV['ITC_TEAM_ID']
team_id(ENV['TEAM_ID']) if ENV['TEAM_ID']
